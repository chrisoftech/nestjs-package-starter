{
  "name": "@chrisoftech/zaap-proto-definitions",
  "version": "1.0.0",
  "description": "NestJS npm package starter",
  "author": "Christian Onwe <chrisoftech@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "main": "dist/test.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@nestjs/common": "^8.2.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/testing": "8.2.6",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.10",
    "@types/supertest": "2.0.11",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "supertest": "6.2.2",
    "ts-jest": "27.1.3",
    "ts-node": "10.4.0",
    "tsc-watch": "4.6.0",
    "tsconfig-paths": "3.12.0",
    "tslint": "6.1.3",
    "typescript": "4.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}